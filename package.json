{
  "name": "sprouting-academy-backend",
  "version": "0.0.1",
  "description": "NestJS backend",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:prod": "nest build --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "start": "nest start",
    "start:dev": "concurrently \"swc src -d dist --config-file .swcrc --watch\" \"nodemon --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,tests}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,tests}/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false --no-file-parallelism ",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "ts-node -r tsconfig-paths/register prisma/seed.ts",
    "db:studio": "prisma studio",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:logs": "docker-compose -f docker/docker-compose.yml logs -f",
    "commit": "commit",
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "deps:check": "bash scripts/update-dependencies.sh all",
    "deps:security": "bash scripts/update-dependencies.sh security",
    "deps:outdated": "bash scripts/update-dependencies.sh outdated",
    "deps:fix": "bash scripts/update-dependencies.sh fix-security",
    "deps:update": "bash scripts/update-dependencies.sh update"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^7.0.1",
    "@scalar/nestjs-api-reference": "^1.0.2",
    "@supabase/supabase-js": "^2.80.0",
    "@types/joi": "^17.2.2",
    "@types/multer": "^2.0.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "multer": "^2.0.2",
    "nanoid": "^5.1.6",
    "nest-winston": "^1.9.4",
    "omise": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.5",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/prompt-cli": "^20.0.0",
    "@eslint/config-array": "^0.21.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@eslint/object-schema": "^2.1.4",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@swc-node/register": "^1.11.1",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.20",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.7.2",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitest/coverage-istanbul": "^4.0.7",
    "@vitest/coverage-v8": "^4.0.7",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "husky": "^8.0.3",
    "lint-staged": "^16.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.0.0",
    "prisma": "^7.0.1",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "unplugin-swc": "^1.5.7",
    "vitest": "^4.0.7"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "hono": "^4.10.7",
    "valibot": "^1.2.0"
  }
}
